CPE = 5

r = (r * (x * y)) * z; /* A2 */
-------------------------------
CPE = 5 * (2/3)

r = r * ((x * y) * z); /* A3 */
-------------------------------
CPE = 5 * (1/3)

r = r * (x * (y * z)); /* A4 */
-------------------------------
CPE = 5 * (1/3)

r = (r * x) * (y * z); /* A5 */
